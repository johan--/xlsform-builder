<%= render partial: 'shared/webform/errors', locals: {target: @form} %>

<%= form_for @form do |helper| %>
  <% @attributes.each do |attrib| %>
    <%= render partial: 'shared/webform/text_field', locals: {
      form: helper,
      attribute: attrib,
      columns: Form::TITLE_COLUMNS
    } %>
  <% end %>
  <%= render partial: 'shared/webform/submit', locals: {form: helper} %>
<% end %>
